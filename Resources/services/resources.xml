<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>

        <service id="sezzle.session_resource"
                 class="SwagPaymentSezzle\SezzleBundle\Resources\SessionResource"
                 public="true">
            <argument type="service" id="sezzle.client_service"/>
        </service>

        <service id="sezzle.capture_resource"
                 class="SwagPaymentSezzle\SezzleBundle\Resources\CaptureResource"
                 public="true">
            <argument type="service" id="sezzle.client_service"/>
        </service>
    </services>
</container>
